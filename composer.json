{
    "name": "wjbecker/connectwise-api-client",
    "description": "Connectwise API wrapper for PHP",
    "keywords": [
        "connectwise"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wjbecker\\ConnectwiseApiClient\\": "src/"
        }
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Wjbecker\\ConnectwiseApiClient\\ConnectWiseServiceProvider"
            ],
            "aliases": {
                "ConnectWise": "Wjbecker\\ConnectwiseApiClient\\Facades\\ConnectWise"
            }
        }
    },
    "authors": [
        {
            "name": "Josh Becker",
            "email": "josh48202@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php" : "^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "spatie/laravel-package-tools": "^1.0|^1.13"
    }
}
